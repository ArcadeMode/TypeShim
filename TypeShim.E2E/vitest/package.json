{
  "name": "@typeshim/e2e-vitest",
  "private": true,
  "type": "module",
  "scripts": {
    "test:node": "cross-env VITE_BROWSER_MODE=false vitest run",
    "test:browser": "cross-env VITE_BROWSER_MODE=true vitest run",
    "initialize": "dotnet publish ../TypeShim.E2E.Wasm -c Release && node ./scripts/empty-map.mjs && npm ci && npx playwright install chromium --with-deps",
    "build": "dotnet publish ../TypeShim.E2E.Wasm -c Release && node ./scripts/empty-map.mjs"
  },
  "dependencies": {
    "@typeshim/e2e-wasm-lib": "file:../wasm-lib-publish/wwwroot"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "@vitest/browser": "^2.1.9",
    "jsdom": "^27.4.0",
    "playwright": "^1.57.0",
    "typescript": "^5.6.0",
    "vite": "^5.4.0",
    "vitest": "^2.1.9"
  }
}
